        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "lint": "eslint src",
        "format": "prettier --write \"src/**/*.{js,css,scss,html}\"",
        "rtl": "webpack",
        "add-locale": "lingui add-locale",
        "i18n-extract": "lingui extract",
        "i18n-compile": "lingui compile"
