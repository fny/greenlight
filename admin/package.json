    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all",
            "ie 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "last 1 edge version",
            "IE 11"
        ]
    },
    "babel": {
        "presets": [
            "env",
            "react",
            "@lingui/babel-preset-react"
        ]
    },
    "lingui": {
        "fallbackLocale": "en",
