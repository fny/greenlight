    "run-cordova-android": "npm run build-cordova && cd cordova && cordova run android",
    "run-cordova-browser": "npm run build-cordova && cd cordova && cordova run browser"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
