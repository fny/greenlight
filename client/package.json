    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "i18n-add-locale": "lingui add-locale",
    "i18n-extract": "lingui extract --clean --overwrite",
