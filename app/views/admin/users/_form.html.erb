<%= simple_form_for [:admin, @user] do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :locale %>
  <%= f.input :password, label: "Password (Leave blank for no change)" %>
  <%= f.input :email %>
  <%= f.input :mobile_number %>
  <%= f.input :zip_code %>
  <%= f.button :submit %>
<% end %>
