<h1>Location Reports</h1>

<p>
  Jump to:
  <a href="#registrations">Registrations</a> |
  <a href="#owners">Owners</a>
</p>


<h2 id="registrations">Registrations</h2>
<table>
  <thead>
    <th>ID</th>
    <td>Name</td>
    <th>Zip Code</th>
    <th>Employee Count</th>
    <th>Registered Count</th>
  </thead>
  <tbody>
    <% @registrations.each do |r| %>
      <tr>
        <td><%= r.id %></td>
        <td>
          <%= link_to r.name, admin_report_path(r.id) %>
        </td>
        <td><%= r.zip_code %></td>
        <td><%= r.employee_count %></td>
        <td><%= r.registered_count %></td>
      </tr>
    <% end %>
  </tbody>
<table>

<h2 id="owners">Owners</h2>
<table>
  <thead>
    <th>Contact</th>
    <th>Location</th>
    <th>Permalink</th>
  </thead>
  <tbody>
    <% @owners.each do |owner| %>
      <tr>
        <td>
          "<%= owner.first_name %> <%= owner.last_name %>" <%= owner.email%>
        </td>
        <td>
          <%= link_to owner.location_name, admin_report_path(owner.location_id) %>
        </td>
        <td>
          <%= link_to owner.permalink, admin_report_path(owner.location_id) %>
        </td>
      </tr>
    <% end %>
  </tbody>
<table>
